# OpenRouter API Key - Get yours at https://openrouter.ai/keys
# This key must start with 'sk-or-'
OPENROUTER_API_KEY="sk-or-v1-your-api-key"

# AI Model to use - Examples:
# OPENROUTER_MODEL="google/gemini-2.0-flash-001"    # Fast responses
# OPENROUTER_MODEL="google/gemini-2.0-pro-exp-02-05:free"      # More capable
# OPENROUTER_MODEL="anthropic/claude-3.7-sonnet" # Most capable
# OPENROUTER_MODEL="openai/o1-mini"    # Balanced
OPENROUTER_MODEL="google/gemini-2.0-flash-001"

# Logging Configuration
# Set to "true" to enable detailed logging of all actions and commands
# Set to "false" to disable logging
LOGGING_ENABLED="false"

# Log file location (absolute path or relative to installation directory)
LOG_FILE="~/.aitermy/logs/aitermy.log"
